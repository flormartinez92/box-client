    "leaflet": "^1.9.4",
    "next": "13.5.6",
    "react": "^18.0.0",
    "react-datepicker": "^4.23.0",
    "react-dom": "^18",
    "react-leaflet": "^4.2.1",
    "react-redux": "^8.1.3",
    "react-toastify": "^10.0.3",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/leaflet": "^1.9.8",
